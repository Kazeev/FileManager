<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.kazeev.jaxrs.fileCRUD.models.AccountEntity" table="account" schema="file_manager"
           catalog="file_manager">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="pass">
            <column name="pass" sql-type="varchar(30)" length="30"/>
        </property>
    </class>
</hibernate-mapping>
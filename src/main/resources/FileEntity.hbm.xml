<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.kazeev.jaxrs.fileCRUD.dao.FileEntity" table="file" schema="file_manager" catalog="FileManager">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar"/>
        </property>
        <property name="extension">
            <column name="extension" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="path">
            <column name="path" sql-type="varchar"/>
        </property>
        <property name="dateUpload">
            <column name="date_upload" sql-type="date"/>
        </property>
        <many-to-one name="accountByIdUserUpload" class="ru.kazeev.jaxrs.fileCRUD.dao.AccountEntity">
            <column name="id_user_upload"/>
        </many-to-one>
    </class>
</hibernate-mapping>